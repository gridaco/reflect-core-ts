{
    "name": "@reflect-ui/core",
    "version": "0.0.5",
    "private": false,
    "scripts": {
        "build": "tsc",
        "prepack": "yarn clean && yarn build",
        "clean": "rimraf dist",
        "test": "tsc"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
