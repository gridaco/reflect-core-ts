    },
    "devDependencies": {
        "@reflect-ui/cg": "^0.0.5",
        "@types/node": "^18.0.6",
        "typescript": "^4.4.4"
    },
    "files": [
        "README.md",
        "LICENSE",
        "dist/index.js",
        "dist/index.d.ts",
        "dist/**/*.js",
        "dist/**/*.d.ts"
